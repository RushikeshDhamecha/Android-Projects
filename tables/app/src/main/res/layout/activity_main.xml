<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0eeed"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/table_of"
        android:layout_width="81dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="181dp"
        android:layout_marginBottom="646dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="number"
        android:textSize="25sp" />

    <EditText
        android:id="@+id/start_txt"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="271dp"
        android:layout_marginBottom="518dp"
        android:gravity="center"
        android:ems="10"
        android:inputType="number"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="136dp"
        android:layout_height="59dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="266dp"
        android:layout_marginBottom="653dp"
        android:text="Table of :"
        android:textColor="#ff0000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="132dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="245dp"
        android:layout_marginBottom="575dp"
        android:text="Start From"
        android:textColor="#ff0000"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="571dp"
        android:text="End To"
        android:textColor="#ff0000"
        android:textSize="25sp" />

    <EditText
        android:id="@+id/end_txt"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="516dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="number"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/display"
        android:layout_width="match_parent"
        android:layout_height="446dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:textSize="25dp"
        android:scrollbars="vertical"
        android:gravity="center"
        android:layout_marginBottom="2dp" />

    <Button
        android:id="@+id/show"
        android:layout_width="138dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="459dp"
        android:text="Show"
        android:background="#2b2b2b"
        android:textColor="#ff2500"
        android:textAllCaps="false"
        android:textSize="25dp" />

</RelativeLayout>